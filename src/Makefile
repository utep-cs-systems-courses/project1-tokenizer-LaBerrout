# tokenizer Makefile #
CFLAGS=-g -O3

# First target is built by default. Usually named "all"
all: tokens

# file dependencies
uimain.o: uimain.c
tokenizer.o: tokenizer.h
#history.o: history.h

# tokenizer_out (target)  ADD history.o ONCE HAVE history.c, and also above ^^^
tokens: uimain.o tokenizer.o
	cc -o $@ $^ $(CFLAGS)

run: tokens
	./tokens

# delete files generated by compilation
clean:
	rm -f *.o tokens


